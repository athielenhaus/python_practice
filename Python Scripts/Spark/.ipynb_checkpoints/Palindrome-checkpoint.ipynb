   "source": [
    "def isPalindrome(x):\n",
    "    varchar = [i for i in x]\n",
    "    print(varchar)\n",
    "    varchar_rev = varchar\n",
    "    varchar_rev.reverse()\n",
    "    print(varchar_rev)\n",
    "    return varchar == varchar_rev"
   ]
